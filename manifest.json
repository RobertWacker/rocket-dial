{
  "manifest_version" : 2,
  "name": "Roket dial",
  "version" : "0.0.1",

  "description" : "",
  "icons": {
    "16": "icon_128x128.png",
    "48": "icon_128x128.png",
    "128": "icon_128x128.png"
  },

  "background" : {"page" : "index.html"},
  "web_accessible_resources" : [
    "common.css"
  ],
  "permissions" : [
    "https://*/*",
    "storage",
    "http://*/*"
  ],
  "content_security_policy": "script-src 'self' https://api.duckduckgo.com https://en.wikipedia.org 'unsafe-eval'; object-src 'unsafe-eval';",
  "chrome_url_overrides" : {
    "newtab": "index.html"
  }
}

